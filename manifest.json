{
  "manifest_version": 2,

  "name": "Validator",
  "description": "This captures text input field from page and pass it for verification ", 
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["jquery.js", "myscript.js"],
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_start",
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}